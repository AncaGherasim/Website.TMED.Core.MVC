        }

    div.moreButton {
        background: #fffecd linear-gradient(to bottom, #fffecd 18%, #fffecd 4%, #ffd968 50%, #ffd968 100%) repeat scroll 0 0;
        border: 1px solid #d9a300;
        color: #000;
        cursor: pointer;
        font-size: 14px !important;
        margin: 0 auto;
        max-width: 240px;
        padding: 10px 15px 10px 20px;
        width: auto;
    }

        div.moreButton:hover {
            background: #fffecd linear-gradient(to top,#fffecd 18%,#fffecd 4%,#ffd968 50%,#ffd968 100%) repeat scroll 0 0
        }

        div.moreButton span {
            float: right;
            font-size: 28px;
            position: relative;
            top: -6px;
            transform: rotate(90deg)
        }
    /* The Modal (background) */
    .modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 1; /* Sit on top */
        padding-top: 100px; /* Location of the box */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    }

    /* Modal Content */

    .byocombo {
        background-color: #fff;
        border: 1px solid #999;
        border-radius: 5px;
        color: #333;
        font-size: 11px;
        outline: 0 none;
        padding: 5px 0 4px 5px;
    }

    .ngtscombo {
        background-color: #fff;
        background-position: right center;
        background-repeat: no-repeat;
        border: 1px solid #999;
        border-radius: 5px;
        color: #333;
        font-size: 11px;
        outline: 0 none;
        padding: 5px 0 3px 1px;
        width: 97%;
    }

    .errorClass {
        border: 2px solid red !important;
        color: red;
    }
</style>

