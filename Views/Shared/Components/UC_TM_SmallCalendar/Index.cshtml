                            <option value="F">First Class</option>
                        </select>
                        <input name="xtxtBYArriving" id="xtxtBYArriving" style="text-align: center" value="" type="hidden">
                    </div>
                    <br style="clear: both" />
                </div>
                <!-- Cities list titles -->
                <div class="dvtranspLabels">
                    <div>Going to:</div>
                    <div>Arriving on:</div>
                    <div>Staying for:</div>
                    <div><b>Travel by:</b></div>
                    <br style="clear: both;" />
                </div>
                <!-- PAX Rooms parameters -->
                <div class="dvtranspRooms">
                    <input type="hidden" name="xiRoom" id="xiRoom" value="1">
                    <input type="hidden" name="xRooms" id="xRooms" value="1">
                    <p>
                        <label for="xiRoomsAndPax">Guest:</label>
                        <select name="xiRoomsAndPax" id="xiRoomsAndPax" onchange="selectRoomPax(this.value)" class="select-css">
                            <option value="1|1">1 room for 1 adult</option>
                            <option value="1|2" selected>1 room for 2 adults</option>
                            <option value="1|3">1 room for 3 adults</option>
                            <option value="1|Other">1 room with children or other options</option>
                            <option value="-1" class="hrDots" disabled></option>
                            <option value="2|2-2">2 rooms for (2 adults + 2 adults)</option>
                            <option value="2|3-3">2 rooms for (3 adults + 3 adults)</option>
                            <option value="2|Other">2 rooms with children or other options</option>
                            <option value="-1" class="hrDots" disabled></option>
                            <option value="3|2-2-2">3 rooms for (2 adults + 2 adults + 2 adults)</option>
                            <option value="3|Other">3 rooms with children or other options</option>
                        </select>
                    </p>
                </div>
                @{
                    string paxAdults = "";
                    for (Int32 p = 0; p <= 6; p++)
                    {
                        if (p == 0)
                        {
                            paxAdults = paxAdults + "<option value=\"" + p + "\" selected>" + p + "</option>";
                        }
                        if (p != 0)
                        {
                            paxAdults = paxAdults + "<option value=\"" + p + "\">" + p + "</option>";
                        }
                    }
                    string paxChild = "";
                    for (Int32 d = 0; d <= 2; d++)
                    {
                        if (d == 0)
                        {
                            paxChild = paxChild + "<option value=\"" + d + "\" selected>" + d + "</option>";
                        }
                        if (d > 0)
                        {
                            paxChild = paxChild + "<option value=\"" + d + "\">" + d + "</option>";
                        }
                    }
                    string childAge = "";
                    for (Int32 c = 1; c <= 11; c++)
                    {
                        if (c == 1)
                        {
                            childAge = childAge + "<option value=\"-1\" selected></option>";
                        }
                        if (c == 2)
                        {
                            childAge = childAge + "<option value=\"" + c + "\">" + c + "</option>";
                        }
                        if (c > 2)
                        {
                            childAge = childAge + "<option value=\"" + c + "\">" + c + "</option>";
                        }
                    }
                }
                <div class="dvtranspAdultChild">
                    <p id="pPaxLabels">
                        <span>&nbsp;</span> <span>Adults:</span> <span>Children:</span> <span id="pChildAges">Child Ages:</span>
                        <br style="clear: both;" />
                    </p>
                    <p id="pRoom1">
                        <span>Room1:</span> <span>
                            <select name="xiAdults" id="xiAdults" onchange="changeAdults(this)" class="select-css">
                                @Html.Raw(@paxAdults)
                            </select>
                        </span><span>
                                   <select name="xiChildren" id="xiChildren" onchange="childAge(this)" class="select-css">
                                       @Html.Raw(@paxChild)
                                   </select>
                        </span><span id="pChildAges1">
                            <label for="xiChild1">Child age 1:</label>
                            <select name="xiChild1" id="xiChild1" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                            <label for="xiChild2">Child age 2:</label>
                            <select name="xiChild2" id="xiChild2" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                        </span>
                        <br style="clear: both;" />
                    </p>
                    <p id="pRoom2">
                        <span>Room2:</span> <span>
                                                <select name="xRoom2_iAdults" id="xRoom2_iAdults" onchange="changeAdults(this)" class="select-css">
                                                    @Html.Raw(@paxAdults)
                                                </select>
                        </span><span>
                                   <select name="xRoom2_iChildren" id="xRoom2_iChildren" onchange="childAge(this)" class="select-css">
                                       @Html.Raw(@paxChild)
                                   </select>
                        </span><span id="pChildAges2">
                            <label for="xRoom2_iChild1">Child age 1:</label>
                            <select name="xRoom2_iChild1" id="xRoom2_iChild1" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                            <label for="xRoom2_iChild2">Child age 2:</label>
                            <select name="xRoom2_iChild2" id="xRoom2_iChild2" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                        </span>
                        <br style="clear: both;" />
                    </p>
                    <p id="pRoom3">
                        <span>Room3:</span> <span>
                                                <select name="xRoom3_iAdults" id="xRoom3_iAdults" onchange="changeAdults(this)" class="select-css">
                                                    @Html.Raw(@paxAdults)
                                                </select>
                        </span><span>
                                   <select name="xRoom3_iChildren" id="xRoom3_iChildren" onchange="childAge(this)" class="select-css">
                                       @Html.Raw(@paxChild)
                                   </select>
                        </span><span id="pChildAges3">
                            <label for="xRoom3_iChild1">Child age 1:</label>
                            <select name="xRoom3_iChild1" id="xRoom3_iChild1" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                            <label for="xRoom3_iChild2">Child age 2:</label>
                            <select name="xRoom3_iChild2" id="xRoom3_iChild2" class="select-css select-css-child">
                                @Html.Raw(@childAge)
                            </select>
                        </span>
                        <br style="clear: both;" />
                    </p>
                </div>
                <div class="dvWording">Please contact us for any special arrangements such as more than three rooms and partial stays.</div>
                <div class="dvContinueButton"><span class="btnContinue">Continue</span></div>
                <input name="xutm_campaign" type="hidden" id="xutm_campaign" value="" />
            </form>
        </div>
    </div>

    <!--- END - Transp Option --->

</div>


