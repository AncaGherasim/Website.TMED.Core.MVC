                                    <div style="padding: 3px 5px 2px 0px; float: left;">
                                        <img src="https://pictures.tripmasters.com/siteassets/d/Plus.jpg" name=@String.Format("picF{0}",otherLn[0]) width="0" height="0" align="absmiddle" id=@String.Format("picF{0}",otherLn[0]) style="cursor: pointer" onclick=@String.Format("openDivInfo({0},'F','{1}','{2}')", otherLn[0], Model.multicountryNa.Replace(" ", "_"), otherLn[1].Replace(" ", "_")) />
