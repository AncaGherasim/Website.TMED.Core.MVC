                                                    <span class="Orange-Arial15NB">
                                                        <b>
                                                            @Model.boxFeaturPacks[fo].STP_Save_.ToString("C0")*
                                                        </b>
                                                    </span>
                                                </div>
                                            }
                                            if (@Model.boxFeaturPacks[fo].NoOfFeed > 0)
                                            {
                                                <div class="Text_Arial11_Blue" align="left" style="padding: 3px 5px 3px 10px;">
                                                </div>
