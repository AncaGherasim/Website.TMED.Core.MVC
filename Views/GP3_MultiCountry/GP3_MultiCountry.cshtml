                                            if (lnGuiNo != "none")
                                            {
                                                <div align="left" id=@String.Format("{0}|{1}|F", lnGuiNo,  @Model.boxFeaturPacks[fo].PDLID) class="Text_12_Gray" style="cursor: pointer;width: 100px; padding: 0px 10px;" onmouseover="showPopUp(this.id)" onmouseout="hidePopUp(this.id)">
                                                    @lnGuiNo
