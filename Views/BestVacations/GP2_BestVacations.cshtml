                                    <div align="left" id=@String.Format("{0}|{1}|F", lnGuiNo, Model.featPack[fo].PDLID) class="Text_12_Gray" style="cursor:pointer; width:100px; padding:0px 15px;" onMouseOver="showPopUp(this.id)" onMouseOut="hidePopUp(this.id)">
                                        @lnGuiNo
                                    </div>
                                }
                            }
