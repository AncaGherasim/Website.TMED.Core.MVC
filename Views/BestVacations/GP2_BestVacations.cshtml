@{
    Int32 maxFeatPack = 0;
    string pack3Ids = "";
    if (Model.featPack.Count < 3)
    {
        maxFeatPack = Model.featPack.Count;
    }
    for (Int32 i = 0; i < maxFeatPack; i++)
    {

        switch (i)
        {
            case 0:
                {
                    pack3Ids = pack3Ids + Model.featPack[i].PDLID.ToString() + ", ";
                    break;
                }
            case 1:
                {
                    pack3Ids = pack3Ids + Model.featPack[i].PDLID.ToString() + ", ";
                    break;
                }
            case 2:
                {
                    pack3Ids = pack3Ids + Model.featPack[i].PDLID.ToString();
                    break;
                }
            case 4:
                {
                    i = maxFeatPack;
                    break;
                }
        }
    }
}


